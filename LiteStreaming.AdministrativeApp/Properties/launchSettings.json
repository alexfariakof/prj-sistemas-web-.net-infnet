{
  "profiles": {
    "Default In Memory": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "InMemory"
      },
      "hotReloadEnabled": true,
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5171;",
      "nativeDebugging": true
    },
    "Default Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "hotReloadEnabled": true,
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7230;http://localhost:5171",
      "nativeDebugging": true
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Docker",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": false,
      "dockerFile": "Dockerfile",
      "DockerfileRunArguments": "-v \"{ProjectDir}/wwwroot:/app/wwwroot\""
    },
    "Docker Development": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Docker",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": false,
      "dockerFile": "Dockerfile.Development",
      "DockerfileRunArguments": "-v \"{ProjectDir}/wwwroot:/app/wwwroot\""
    },
    "Docker Production": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Docker",
        "ASPNETCORE_HTTP_PORTS": "80;443"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "dockerFile": "Dockerfile.Production",
      "DockerfileRunArguments": "-v \"{ProjectDir}/wwwroot:/app/wwwroot\""
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": true,
      "launchUrl": "https://localhost:7230",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7230;http://localhost:5171"
      },
      "distributionName": ""
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": true,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:16400",
      "sslPort": 7230
    }
  }
}